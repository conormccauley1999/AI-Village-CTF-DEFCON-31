   "source": [
    "import requests\n",
    "\n",
    "def query(data):\n",
    "    return requests.post(\n",
    "        \"http://cluster2.advml.com/score\",\n",
    "        json={ \"data\": data }\n",
    "    ).json()\n",
    "\n",
    "for i in range(1, 10):\n",
    "    res = query(i)\n",
    "    if \"flag\" in res:\n",
    "        print(i, res[\"flag\"])\n",
    "        break"
   ]
  }
